{
  "name": "ctab-page",
  "version": "1.0.0",
  "description": "A web extension giving you a customizable new tab screen",
  "keywords": [
    "extension",
    "newtab",
    "dashboard"
  ],
  "license": "MIT",
  "bugs": "https://github.com/clanghout/CTab-Page/issues",
  "repository": "github:clanghout/CTab-Page",
  "author": "clanghout",
  "contributors": [
    "foresterre"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "parcel src/index.html",
    "start": "node scripts/createSizeStyles.js && webpack --mode=development",
    "build": "node scripts/createSizeStyles.js && webpack --mode=production",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "big-text.js-patched": "^1.0.1",
    "muuri": "^0.9.5",
    "rome": "^10.0.4-beta",
    "streamsaver": "2.0.5",
    "vanilla-picker": "^2.11.2"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-extension-manifest-plugin": "^0.8.0"
  },
  "private": true
}
