{
  "name": "ctab-page",
  "version": "1.0.0",
  "description": "A web extension giving you a customizable new tab screen",
  "keywords": [
    "extension",
    "newtab",
    "dashboard"
  ],
  "license": "MIT",
  "bugs": "https://github.com/clanghout/CTab-Page/issues",
  "repository": "github:clanghout/CTab-Page",
  "author": "clanghout",
  "contributors": [
    "foresterre"
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/index.html",
  "man": "README.md",
  "scripts": {
    "dev": "parcel src/index.html",
    "start": "rm -rf dist/ && mkdir dist && node scripts/createSizeStyles.js && parcel src/index.html",
    "build": "rm -rf dist/ && mkdir dist && node scripts/createSizeStyles.js && parcel build src/manifest.json",
    "clean": "rm -rf dist/ && mkdir dist",
    "typecheck": "tsc --noEmit",
    "updateStuff": "npx browserslist@latest --update-db",
    "upgrade-latest": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "big-text.js-patched": "^1.0.1",
    "lit": "^2.0.2",
    "muuri": "^0.9.5",
    "rome": "^10.0.4-beta",
    "streamsaver": "2.0.5",
    "vanilla-picker": "^2.11.2",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-web-extension": "^1.6.1",
    "sass": "^1.43.4",
    "typescript": "^4.4.4"
  },
  "private": true
}
