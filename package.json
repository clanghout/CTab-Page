{
  "name": "ctab-page",
  "version": "1.0.0",
  "description": "A web extension giving you a customizable new tab screen",
  "keywords": [
    "extension",
    "newtab",
    "dashboard"
  ],
  "license": "MIT",
  "bugs": "https://github.com/clanghout/CTab-Page/issues",
  "repository": "github:clanghout/CTab-Page",
  "author": "clanghout",
  "contributors": [
    "foresterre"
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "man": "README.md",
  "scripts": {
    "dev": "parcel src/index.html",
    "start": "rm -rf dist/ && mkdir dist && node scripts/createSizeStyles.js && parcel src/index.html",
    "build": "rm -rf dist/ && mkdir dist && node scripts/createSizeStyles.js && parcel build src/manifest.json --dist-dir dist",
    "clean": "rm -rf dist/ && mkdir dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "big-text.js-patched": "^1.0.1",
    "muuri": "^0.9.5",
    "rome": "^10.0.4-beta",
    "streamsaver": "2.0.5",
    "vanilla-picker": "^2.11.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@types/node": "^16.4.7",
    "parcel": "^2.0.0-beta.2",
    "parcel-plugin-web-extension-2": "^2.0.3",
    "sass": "^1.36.0",
    "typescript": "^4.3.5"
  },
  "private": true
}
